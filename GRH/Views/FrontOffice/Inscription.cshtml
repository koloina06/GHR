         <!-- Basic -->
        <div class="ex-form-1 pt-5 pb-5">
            <div class="container">
                <div class="row">
                    <div class="col-xl-6 offset-xl-3">
                        <div class="text-box mt-5 mb-5">
                            <p class="mb-4">Remplir les case suivantes pour s'inscrire'. Déjà inscrit?<a class="blue" href="https://localhost:7248/FrontOffice">Connectez-vous</a></p>

                            <!-- Sign Up Form -->
                            <form action="https://localhost:7248/FrontOffice/Insc/" method="POST">
                                <div class="mb-4 form-floating">
                                    <input type="email" class="form-control" name="email" id="floatingInput" placeholder="Adresse Email">
                                    <label for="floatingInput">Email</label>
                                </div>
                                <div class="mb-4 form-floating">
                                    <input type="text" class="form-control" id="floatingInput2" placeholder="Nom" name="nom">
                                    <label for="floatingInput">Nom</label>
                                </div>
                                  <div class="mb-4 form-floating">
                                    <input type="text" class="form-control" id="floatingInput2" placeholder="Prenom" name="prenom">
                                    <label for="floatingInput">Prenom</label>
                                </div>
                                <div class="mb-4 form-floating">
                                    <input type="date" name="dtn" class="form-control" id="floatingPassword">
                                    <label for="floatingPassword">Date de Naissance</label>
                                </div>
                                <div class="mb-4 form-floating">
                                    <input type="text" class="form-control" id="floatingInput2" placeholder="Adresse" name="adresse">
                                    <label for="floatingInput">Adresse</label>
                                </div>
                                <div class="mb-4 form-floating">
                                    <input type="text" class="form-control" id="floatingInput2" placeholder="Password" name="mdp">
                                    <label for="floatingInput">Mot de passe</label>
                                </div>
                                <div class="mb-4 form-floating">
                                    <select name="sexe"  class="form-control">
                                        <option value="0">Homme</option>
                                        <option value="1">Femme</option>
                                    </select>
                                </div>
                               
                                <input type="submit" value="Sign up" class="form-control-submit-button" />
                            </form>
                            <!-- end of sign up form -->

                        </div> <!-- end of text-box -->
                    </div> <!-- end of col -->
                </div> <!-- end of row -->
            </div> <!-- end of container -->
